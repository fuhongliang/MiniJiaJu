    justify-content: center;
    width: 100%;
    height: 100%;
}

.check-icon {
    width: 40rpx;
    height: 40rpx;
    border: 4rpx solid #ccc;
    border-radius: 1000rpx;
    position: relative;
    margin-right: 4rpx;
}

.check-icon::after {
    position: absolute;
    content: " ";
    display: block;
    width: 28rpx;
    height: 28rpx;
    top: 2rpx;
    left: 2rpx;
    background: #e3e3e3;
    border-radius: 1000rpx;
}

.active .check-icon::after {
    background: #ff4544;
}

.active .check-icon {
    border-color: #ff4544;
}

