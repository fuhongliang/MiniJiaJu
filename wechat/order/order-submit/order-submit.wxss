.cart-list {
    margin-bottom: 30rpx;
}

.cart-checkbox {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,);
}

.cart-checkbox.active {
    background-image: url(data:image/png;base64,);
}

.cart-item {
    background: #fff;
    padding: 30rpx 30rpx 10rpx 30rpx;
}

.cart-item .goods-pic {
    width: 154rpx;
    height: 154rpx;
    margin-right: 20rpx;
}

.cart-item .goods-name {
    width: 500rpx;
    margin-bottom: 10rpx;
    overflow: hidden;
}

.cart-item .attr-list,.cart-item .num {
    font-size: 28rpx;
    color: #999999;
}

.cart-item .attr-list .attr-item {
    display: inline-block;
    margin-right: 36rpx;
}

.cart-item .attr-list .attr-item:last-child {
    margin-right: 0;
}

.coupon-picker {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1001;
    background: #fff;
    border-bottom:0.5px solid #F2F2F2;
}

.coupon-list {
    padding: 34rpx;
}

.coupon-list .coupon-item {
    height: 152rpx;
    width: 682rpx;
    position: relative;
    margin-bottom: 24rpx;
}

.coupon-list .coupon-item.coupon-status-1 .coupon-right,.coupon-list .coupon-item.coupon-status-2 .coupon-right {
    color: rgba(0,0,0,0.35)!important;
}

.coupon-list .coupon-item .coupon-bg {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: -1;
}

.coupon-list .coupon-item .coupon-status-icon {
    width: 140rpx;
    height: 98rpx;
    position: absolute;
    top: 0;
    right: 8rpx;
    z-index: 1;
}

.coupon-list .coupon-item .coupon-left {
    color: #fff;
    width: 202rpx;
}

.coupon-list .coupon-item .coupon-right {
    padding: 20rpx 10rpx;
}

.active {
    color: #F61616;
}

.shop-block {
