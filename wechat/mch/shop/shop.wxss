.header { border-bottom: 1rpx solid #e3e3e3; height: 200rpx; position: relative; }
.header .header-bg{ position: absolute; left: 0; top: 0; width: 100%; height: 100%; z-index: -1; }
.header > wx-view { width: 100%; padding-bottom: 24rpx; }
.header-text { color: rgba(255, 255, 255, 0.85); text-shadow: 0 1rpx 3rpx rgba(0, 0, 0, 0.5); }
.header .favorite{ background: #ff4544; color: #fff; font-size: 9pt; border-radius: 999rpx 0 0 999rpx; height: 64rpx; padding: 0 20rpx; }
.header .favorite .favorite-text{ text-align: center; margin-left: 16rpx; }
.tab{font-size: 9pt; border-bottom: 1rpx solid #e3e3e3; height: 120rpx; overflow: hidden; margin-top: 30rpx;}
.tab-item{ text-align: center; height: 120rpx; color: #888; }

.tab-item .classify_title{
  font-size:34rpx;
  font-weight:500;
  color:#666666;
}
.tab-item.active .classify_title{
  font-size:34rpx;
  font-weight:bold;
  color:#000000;
}
.tab-item.active{
  border-bottom:4rpx solid #0EC262;
}
.tab-item wx-image{ width: 48rpx; height: 48rpx; }
.fixed-tab-1 .tab, .fixed-tab-3 .tab { position: fixed; top:0; left: 0; width: 100%; z-index: 10; margin: 0;}
.fixed-tab-1.after-navber, .fixed-tab-3.after-navber { padding-top: 120rpx; }
.coupon-list{ width: 100%; height: 170rpx; padding: 20rpx 24rpx; }
.coupon-list > wx-view { flex-wrap: nowrap; }
.coupon-list .coupon-item{ width: 256rpx; height: 130rpx; display: inline-block; margin-right: 16rpx; position: relative; color: #fff; }
.coupon-list .coupon-item:last-child{ margin-right: 0; }
.coupon-list .coupon-item wx-image{ position: absolute; left: 0; right: 0; width: 100%; height: 100%; z-index: -1; }
.coupon-list .coupon-item > wx-view{ height: 100%; }
.coupon-list .coupon-item .coupon-money{ height: 80rpx; line-height: 80rpx; width: 200rpx; text-align: center; white-space: nowrap; overflow: hidden; }
.coupon-list .coupon-item .coupon-limit{ height: 50rpx; line-height: 50rpx; font-size: 7pt; width: 200rpx; text-align: center; white-space: nowrap; overflow: hidden; }
.coupon-list .coupon-item .get-coupon-now{ width: 55rpx; height: 100%; font-size: 7pt; word-break: break-all; padding: 10rpx; line-height: 1.25; text-align: center; }
.img_container{
    width: 330rpx; 
    height:330rpx;
    margin-bottom: 20rpx;
    border:1rpx solid #F2F2F2;
}
.img_container image{
    width: 320rpx;
    height:320rpx;
}

.list-header{ padding: 0 !important; height: 90rpx; }
.list-header > wx-navigator wx-image{ width: 24rpx; height: 24rpx; margin-left: 12rpx; }
.list-header > wx-navigator.active{ color: #ff4544; }
.new-goods-header{ margin: 12rpx 0; color: #888; font-size: 9pt; }
.new-goods-header .flex-grow-1 wx-view{ display: block; height: 1rpx; background: #e3e3e3; width: 100%; margin: 0 30rpx; }
