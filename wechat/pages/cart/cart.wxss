.bottom-bar .submit-btn {
    background: #0EC262;
    width: 160rpx;
    text-align: center;
}

.bottom-bar .edit-btn {
    background: #F7C62F;
    width: 160rpx;
    text-align: center;
}

.bottom-bar {
    position: fixed;
    left: 0;
    height: 100rpx;
    background: #fff;
    width: 100%;
    z-index: 1000;
}
.page.show_navbar .body .bottom-bar {
    bottom:98rpx;
}

.cart-list {
    padding-bottom: 24rpx;
}

.cart-checkbox {
    display: inline-block;
    width: 34rpx;
    height: 34rpx;
    background-size: 100% 100%;
    background-image: url(data:image/png;base64,)
}

.cart-checkbox.active {
    background-image: url(data:image/png;base64,)
}

.cart-item {
    background: #fff;
    padding: 30rpx 24rpx 30rpx 0;
}

.cart-item .goods-pic {
    width: 160rpx;
    height: 160rpx;
    margin-right: 20rpx;
}

.cart-item .goods-name navigator{
    background: none;
}
.cart-item .goods-name {
    margin-bottom: 10rpx;
    -webkit-line-clamp: 1;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;
    color: #333333;
    font-size: 34rpx;
}

.sort_border{
    border-top:1rpx solid #F2F2F2;
}

.cart-item .attr-list .attr-item {
    display: inline-block;
    margin-right: 36rpx;
}

.cart-item .attr-list .attr-item:last-child {
    margin-right: 0;
}

.cart-item .cart-tag {
    background: #ffd3d6;
    padding: 3rpx 6rpx;
}

.page.show_navbar.device_iphone_x .body .bottom-bar {
    bottom: 162rpx;
}

.image1 {
    width: 36rpx;
    height: 36rpx;
}

.row-data {
    height: 36rpx;
    margin: 0 4rpx;
    width: 36rpx;
    text-align: center;
}

.mch-header {
    background: #fff;
}

.business_avatar{
    width: 40rpx;
    height: 40rpx;
    border-radius:50%;
    margin-right: 15rpx;
    border:1rpx solid #F2F2F2;
}

.arrow{
    width: 10rpx;
    height: 16rpx;
    margin-left: 18rpx;
}
.goods-num-lacked{
  transform: translateY(16rpx);
}
